    "beforeDevCommand": "cd app && npm run dev",
    "devPath": "http://localhost:5173",
        "distDir": "app/dist"
    },
    "tauri": {
        "bundle": {
            "identifier": "sfz.plugin_manager",
            "icon": ["icon.png"]
        },
